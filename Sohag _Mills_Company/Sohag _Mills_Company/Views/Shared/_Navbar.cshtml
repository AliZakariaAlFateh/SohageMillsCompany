                TimeStatussss = TimeState[0]
            else
                TimeStatussss = TimeState[1]
            $("#_Time").empty();
            $("#_Time").append(`   ${currentTime1}`)
            $("#_Status").empty();
            $("#_Status").append(`${TimeStatussss} `)
