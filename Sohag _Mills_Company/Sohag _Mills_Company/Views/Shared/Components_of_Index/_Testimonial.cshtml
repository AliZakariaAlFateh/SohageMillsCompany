<!-- Testimonial Start -->
@model IEnumerable<CampanyTeam>


@if (Model.Any())
{
    <div class="container-xxl py-5" id="oldmemebers">
        <div class="container">
            <div class="text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 600px;">
                <h6 class="text-primary"></h6>
                <h1 class="mb-4">الأعضاء السابقين</h1>
            </div>
            <div class="owl-carousel testimonial-carousel wow fadeInUp" data-wow-delay="0.1s">

                @foreach (var teamMember in Model)
                {
                    <div class="testimonial-item text-center">
                        <div class="testimonial-img position-relative">
                            <img class="img-fluid rounded-circle mx-auto mb-5" src="Images/@teamMember.ImageName">
                            <!--
                            <div class="btn-square bg-primary rounded-circle">
                                <i class="fa fa-quote-left text-white"></i>
                            </div>
                            -->


                        </div>
                        <div class="testimonial-text text-center rounded p-4">
                            <p>@teamMember.Representation_Body</p>
                            <h5 class="mb-1">@teamMember.nickname</h5>
                            <span class="fst-italic">@teamMember.Postion</span>
                        </div>
                    </div>
                }

            </div>
        </div>
    </div>
}
else

{


    <div class="container-xxl py-5" id="oldmemebers">
        <div class="container">
            <div class="text-center mx-auto mb-5 wow fadeInUp" data-wow-delay="0.1s" style="max-width: 600px;">
                <h6 class="text-primary"></h6>
                <h1 class="mb-4">الأعـــــــضـــاء الـــســـابــقـيــن</h1>
            </div>
            <div class="owl-carousel testimonial-carousel wow fadeInUp" data-wow-delay="0.1s">


                    <div class="testimonial-item text-center">
                        <div class="testimonial-img position-relative">
                            <img class="img-fluid rounded-circle mx-auto mb-5" src="~/img/testimonial-2.jpg">
                            <!--
                        <div class="btn-square bg-primary rounded-circle">
                            <i class="fa fa-quote-left text-white"></i>
                        </div>
                            -->


                        </div>
                        <div class="testimonial-text text-center rounded p-4">
                            <p>Our Site For Showing all our Services.</p>
                            <h5 class="mb-1">EAgle</h5>
                            <span class="fst-italic">Member</span>
                        </div>
                    </div>
                

            </div>
        </div>
    </div>
    

}



